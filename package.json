  "main": "server.js",
  "scripts": {
    "start": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate --env production && node server.js",
    "dev": "nodemon server.js",

    "migrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate --env production",
    "migrate:undo": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all --env production",

    "seed": "node ./node_modules/sequelize-cli/lib/sequelize db:seed:all --env production",
    "seed:undo": "node ./node_modules/sequelize-cli/lib/sequelize db:seed:undo:all --env production",
    
    "reset-db": "node ./node_modules/sequelize-cli/lib/sequelize db:drop --env production && node ./node_modules/sequelize-cli/lib/sequelize db:create --env production && node ./node_modules/sequelize-cli/lib/sequelize db:migrate --env production"
  },
